<div class="tabcontent" id="Courses">
  <table class="table">
    <tr>
      <th>Titulo</th>
      <th>Autor</th>
      <th>Id</th>
      <th></th>
    </tr>
    {{#allAuthors}}
    {{#courses}}
    <tr>
      <td>{{title}}</td>
      <td>{{author}}</td>
      <td id="id">{{_id}}</td>
      <td>
        <button id='deleteCourse' type="button" name="delete">Delete</button>
        <button id="editCourse" type="button" name="edit">Edit</button>
      </td>
    </tr>
    {{/courses}}
    {{/allAuthors}}
  </table>
</div>
<script language="JavaScript" type="text/javascript" src="/js/deleteCourse.js"></script>
